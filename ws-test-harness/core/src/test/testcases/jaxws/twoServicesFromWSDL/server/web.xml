<web-app>
    <display-name>jaxws.twoServices</display-name>
    <description>Tests two @WebService with fromjava scenario.</description>
    <listener>
        <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>jaxws.twoServices</servlet-name>
        <display-name>jaxws.twoServices</display-name>
        <description>Tests two @WebService with fromjava scenario.</description>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>jaxws.twoServices</servlet-name>
        <url-pattern>/AddNumbersImpl2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>jaxws.twoServices</servlet-name>
        <url-pattern>/AddNumbersImpl1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>60</session-timeout>
    </session-config>
</web-app>
